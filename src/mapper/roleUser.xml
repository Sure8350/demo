<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.ru">
	<insert id="save" parameterType="com.ys.entity.RoleUser">
		INSERT INTO
		role_user	(user_id,
					role_id)
		VALUES	(#{userId},
				#{roleId})
	</insert>
	
</mapper>